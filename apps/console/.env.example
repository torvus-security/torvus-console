# Supabase connection (Console uses service role on the server)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=anon-key
SUPABASE_SERVICE_ROLE=service-role-key

# Cloudflare Access JWT verification
CF_ACCESS_JWT_ISS=https://your-team.cloudflareaccess.com
CF_ACCESS_JWT_AUD=00000000-0000-0000-0000-000000000000

# Staff UI session requirement (set to true to require Supabase staff sessions)
FEATURE_REQUIRE_STAFF_SESSION=true

# Platform integrations
TORVUS_PLATFORM_URL=https://platform.torvussecurity.com
TORVUS_PLATFORM_STATS_URL=https://platform.torvussecurity.com/api/stats
TORVUS_RELEASE_SIMULATOR_URL=

# Feature guards
TORVUS_FEATURE_ENABLE_RELEASE_EXECUTION=0

# Release executor authorization
RELEASE_EXECUTOR_SIGNING_KEY=

# Observability / analytics
NEXT_PUBLIC_POSTHOG_HOST=
NEXT_PUBLIC_POSTHOG_KEY=

# Statuspage embed configuration
NEXT_PUBLIC_STATUSPAGE_PAGE_ID=
NEXT_PUBLIC_STATUSPAGE_URL=
